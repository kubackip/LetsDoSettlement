<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kubackip.github.io.LetsDoSettlement.AddMemberController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Plik">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#backToMain" text="Powrót do głównego okna" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edycja">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#clearAllTheTextFields" text="Wyczyść wszystkie pola" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Pomoc">
          <items>
            <MenuItem mnemonicParsing="false" text="Jak używać aplikacji" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <TextField fx:id="nameTextField" layoutX="27.0" layoutY="51.0" prefHeight="42.0" prefWidth="592.0">
          <font>
            <Font size="14.0" />
          </font>
        </TextField>
        <TextField fx:id="secondNameTextField" layoutX="27.0" layoutY="144.0" prefHeight="42.0" prefWidth="592.0">
          <font>
            <Font size="14.0" />
          </font>
        </TextField>
        <Button fx:id="addMemberButton" layoutX="198.0" layoutY="255.0" mnemonicParsing="false" onAction="#addMember" prefHeight="42.0" prefWidth="244.0" text="Dodaj użytkownika" />
        <Label layoutX="27.0" layoutY="24.0" prefHeight="16.0" prefWidth="91.0" text="Podaj Imię" />
        <Label layoutX="27.0" layoutY="120.0" prefHeight="16.0" prefWidth="139.0" text="Podaj Nazwisko" />
      </children>
    </AnchorPane>
  </children>
</VBox>
